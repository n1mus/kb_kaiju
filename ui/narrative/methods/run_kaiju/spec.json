{
	"ver": "1.1.0",

	"authors": [
		"dylan","seanjungbluth"
	],
	"contact": "http://kbase.us/contact-us/",
	"visible": true,
	"categories": ["active", "communities" ],
	"widgets": {
		"input": null,
		"output": "no-display"
	},
	"parameters": [
		{
			"id": "input_refs",
			"optional": false,
			"advanced": false,
			"allow_multiple": true,
			"default_values": [ "" ],
			"field_type": "text",
			"text_options": {
				"valid_ws_types": [ "KBaseSets.ReadsSet", "KBaseFile.PairedEndLibrary", "KBaseFile.SingleEndLibrary", "KBaseAssembly.PairedEndLibrary", "KBaseAssembly.SingleEndLibrary" ]
			}
		},
		{
			"id": "tax_levels",
			"optional": false,
			"advanced": false,
			"allow_multiple": true,
			"default_values": [ "ALL" ],
		        "field_type": "dropdown",
		        "dropdown_options": {
					"options": [
						{
							"value": "ALL",
							"display": "ALL",
							"id": "ALL",
							"ui-name": "ALL"
						},
						{
							"value": "phylum",
							"display": "phylum",
							"id": "phylum",
							"ui-name": "phylum"
						},
						{
							"value": "class",
							"display": "class",
							"id": "class",
							"ui-name": "class"
						},
						{
							"value": "order",
							"display": "order",
							"id": "order",
							"ui-name": "order"
						},
						{
							"value": "family",
							"display": "family",
							"id": "family",
							"ui-name": "family"
						},
						{
							"value": "genus",
							"display": "genus",
							"id": "genus",
							"ui-name": "genus"
						},
						{
							"value": "species",
							"display": "species",
							"id": "species",
							"ui-name": "species"
						}
					]
				}
		},
		{
			"id": "db_type",
			"optional": false,
			"advanced": false,
			"allow_multiple": false,
			"default_values": ["kaiju_index"],
			"field_type": "dropdown",
				"dropdown_options": {
					"options": [
						{
							"value": "kaiju_index",
							"display": "RefSeq Genomes (no Euks)",
							"id": "kaiju_index",
							"ui-name": "kaiju_index"
						},
						{
							"value": "kaiju_index_pg",
							"display": "proGenomes (no Euks)",
							"id": "kaiju_index_pg",
							"ui-name": "kaiju_index_pg"
						},
						{
							"value": "kaiju_index_nr",
							"display": "NCBI BLAST nr (no Euks)",
							"id": "kaiju_index_nr",
							"ui-name": "kaiju_index_nr"
						},
						{
							"value": "kaiju_index_nr_euk",
							"display": "NCBI BLAST nr+euk",
							"id": "kaiju_index_nr_euk",
							"ui-name": "kaiju_index_nr_euk"
						}
					]
				}
		},
		{
			"id": "filter_percent",
			"optional": false,
			"advanced": false,
			"allow_multiple": false,
			"default_values": [ "0.5" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "float",
			        "min_float": 0.0,
			        "max_float": 10.0
			}
		},
		{
			"id": "subsample_percent",
			"optional": true,
			"advanced": false,
			"allow_multiple": false,
			"default_values": [ "10" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int",
			        "min_int": 1,
			        "max_int": 100
			}
		},
		{
			"id": "subsample_replicates",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "1" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int",
			        "min_int": 1
			}
		},
		{
			"id": "subsample_seed",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "1" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "seg_filter",
			"optional": false,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ 1 ],
			"field_type": "dropdown",
				"dropdown_options": {
					"options": [
						{
							"value": 1,
							"display": "filter",
							"id": "filter",
							"ui-name": "filter"
						},
						{
							"value": 0,
							"display": "no filter",
							"id": "no_filter",
							"ui-name": "no_filter"
						}
					]
				}
		},
		{
			"id": "min_match_length",
			"optional": false,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "11" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int",
			        "min_int" : 9
			}
		},
		{
			"id": "greedy_run_mode",
			"optional": false,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ 1 ],
			"field_type": "dropdown",
				"dropdown_options": {
					"options": [
						{
							"value": 1,
							"display": "allow mismatches (Greedy mode)",
							"id": "greedy",
							"ui-name": "greedy"
						},
						{
							"value": 0,
							"display": "maximum exact match (MEM mode)",
							"id": "perfect",
							"ui-name": "perfect"
						}
					]
				}
		},
		{
			"id": "greedy_allowed_mismatches",
			"optional": false,
			"advanced": true,
			"allow_multiple": false,
			"default_values": ["5"],
			"field_type": "text",
			"text_options": {
				"validate_as": "int",
			        "min_int": 1
			}
		},
		{
			"id": "greedy_min_match_score",
			"optional": false,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "75" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int",
			        "min_int": 10
			}
		},
		{
			"id": "greedy_max_e_value",
			"optional": false,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "0.05" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "float",
			        "min_float": 0.0,
			        "max_float": 1.0
			}
		},
		{
			"id": "sort_taxa_by",
			"optional": false,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "totals" ],
			"field_type": "dropdown",
				"dropdown_options": {
					"options": [
						{
							"value": "totals",
							"display": "total abundance",
							"id": "sort_totals",
							"ui-name": "sort_totals"
						},
						{
							"value": "alpha",
							"display": "alphabetically by taxa names",
							"id": "sort_alpha",
							"ui-name": "sort_alpha"
						}
					]
				}
		}
	],

	"behavior": {
		"service-mapping": {
			"url": "",
			"name": "kb_kaiju",
			"method": "run_kaiju",
			"input_mapping": [
				{
					"narrative_system_variable": "workspace",
					"target_property": "workspace_name"
				},
				{
					"input_parameter": "input_refs",
					"target_property": "input_refs",
					"target_type_transform": "list<resolved-ref>"

				},
				{
					"input_parameter": "tax_levels",
					"target_property": "tax_levels"
				},
				{
					"input_parameter": "db_type",
					"target_property": "db_type"
				},
				{
					"input_parameter": "filter_percent",
					"target_property": "filter_percent"
				},
				{
					"input_parameter": "subsample_percent",
					"target_property": "subsample_percent"
				},
				{
					"input_parameter": "subsample_replicates",
					"target_property": "subsample_replicates"
				},
				{
					"input_parameter": "subsample_seed",
					"target_property": "subsample_seed"
				},
				{
					"input_parameter": "seg_filter",
					"target_property": "seg_filter"
				},
				{
					"input_parameter": "min_match_length",
					"target_property": "min_match_length"
				},
				{
					"input_parameter": "greedy_run_mode",
					"target_property": "greedy_run_mode"
				},
				{
					"input_parameter": "greedy_allowed_mismatches",
					"target_property": "greedy_allowed_mismatches"
				},
				{
					"input_parameter": "greedy_min_match_score",
					"target_property": "greedy_min_match_score"
				},
				{
					"input_parameter": "greedy_max_e_value",
					"target_property": "greedy_max_e_value"
				},
				{
					"input_parameter": "sort_taxa_by",
					"target_property": "sort_taxa_by"
				}
			],
			"output_mapping": [
				{
					"service_method_output_path": [0, "report_name"],
					"target_property": "report_name"
				},
				{
					"service_method_output_path": [0, "report_ref"],
					"target_property": "report_ref"
				},
				{
					"narrative_system_variable": "workspace",
					"target_property": "workspace_name"
				}
			]
		}
	},
	"job_id_output_field": "docker"
}
